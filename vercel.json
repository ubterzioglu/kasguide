{
  "buildCommand": null,
  "outputDirectory": ".",
  "functions": {
    "api/admin/**/*.js": {
      "memory": 1024,
      "maxDuration": 10
    },
    "api/**/*-submit.js": {
      "memory": 1024,
      "maxDuration": 30
    },
    "api/**/*.js": {
      "memory": 512,
      "maxDuration": 10
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PATCH, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "X-API-Key, Content-Type"
        }
      ]
    }
  ]
}
